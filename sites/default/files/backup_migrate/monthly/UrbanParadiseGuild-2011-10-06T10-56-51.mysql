INSERT INTO `node_revisions` VALUES ('340','341','4','Vizcaya Thursdays - 4/7/11','NOTE:\r\n\r\nNursery Steward Carrie needs more physical therapy than expected, so this activity will resume next week, 4/14/11.\r\nSorry for any inconvenience. \r\n\r\nWe will continue with Vizcaya 2nd Saturday as planned on 4/9/11.  \r\nPlease see http://www.urban-paradise.org/040911 .','NOTE:\r\n\r\nNursery Steward Carrie needs more physical therapy than expected, so this activity will resume next week, 4/14/11.\r\nSorry for any inconvenience. \r\n\r\nWe will continue with Vizcaya 2nd Saturday as planned on 4/9/11.  \r\nPlease see http://www.urban-paradise.org/040911 .','','1302126026','1'),('349','350','4','Organic Stewardship Innovation Contest!','This page will be updated soon with rules and an application format.\r\n_________________________________________________\r\nSubmit your idea for new methods of Organic Stewardship.\r\n_________________________________________________\r\n\r\nWinners will receive Innovation Micro-Grants of the Materials and Tools they need to test their methods!\r\n\r\nThe proposals will be just 1 page, and the contest will be judged by Restoration Biologists, Earth-Science Professionals, UPG Partners and UPG Staffers who do Organic Stewardship on a weekly basis.\r\n\r\nFindings (what works or doesn\'t) will be published as part of the UPG Manual.\r\n\r\nFunding is provided by the DOF: Florida Agriculture Department\'s Division of Forestry.  THANKS! ','This page will be updated soon with rules and an application format.\r\n_________________________________________________\r\nSubmit your idea for new methods of Organic Stewardship.\r\n_________________________________________________\r\n\r\nWinners will receive Innovation Micro-Grants of the Materials and Tools they need to test their methods!\r\n\r\nThe proposals will be just 1 page, and the contest will be judged by Restoration Biologists, Earth-Science Professionals, UPG Partners and UPG Staffers who do Organic Stewardship on a weekly basis.\r\n\r','','1303245790','1');
