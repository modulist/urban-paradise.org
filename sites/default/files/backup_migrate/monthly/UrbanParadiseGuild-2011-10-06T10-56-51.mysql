INSERT INTO `node_revisions` VALUES ('367','368','4','Vizcaya Thursdays - 5/5/11','Thursday is the UPG Vizcaya Nursery Day!\r\n\r\nANNOUNCEMENT\r\nUPG Vizcaya needs YOU!  We have vacancies for:\r\nInterns (College & HS)\r\nTeam Leaders\r\nVolunteer Staff\r\nInstructors\r\n\r\nLEADER\r\nCarrie Cleland, UPG Vizcaya Nursery Steward\r\n\r\nMISSION\r\nUPG maintains the Vizcaya Nursery, which includes both the UPG and Vizcaya collections.\r\nMaintain the UPG Vizcaya Nursery. Transplant native plants, weed them, and care for them as needed.\r\n\r\nRSVP\r\nRSVP is REQUIRED by Wednesday to participate. Please send to to sam@urban-paradise.org.\r\nIf you show up at Vizcaya without an RSVP, you will not find the Nursery or be allowed to participate.\r\nThis is required by our Partner, which is a unit of Miami-Dade County Government.\r\n\r\nSCHEDULE\r\n5:30pm until Sunset\r\n\r\nBRING / WEAR\r\nClothes that can get grubby\r\nClosed-toed shoes or boots\r\nGardening gloves (optional)\r\nTrowel (optional)\r\nBug spray (just in case)\r\n\r\nMORE INFO & DIRECTIONS\r\nhttp://www.urban-paradise.org/Vizcaya\r\n\r\nNOTE\r\nUPG Vizcaya Monthly Work-Days occur on the 2nd Saturday of every month.','Thursday is the UPG Vizcaya Nursery Day!\r\n\r\nANNOUNCEMENT\r\nUPG Vizcaya needs YOU!  We have vacancies for:\r\nInterns (College & HS)\r\nTeam Leaders\r\nVolunteer Staff\r\nInstructors\r\n\r\nLEADER\r\nCarrie Cleland, UPG Vizcaya Nursery Steward\r\n\r\nMISSION\r\nUPG maintains the Vizcaya Nursery, which includes both the UPG and Vizcaya collections.\r\nMaintain the UPG Vizcaya Nursery. Transplant native plants, weed them, and care for them as needed.\r\n\r\nRSVP\r\nRSVP is REQUIRED by Wednesday to participate. Please send to to sam@urban-paradise.org.\r','','1304000426','1'),('368','369','4','Vizcaya Thursdays - 5/12/11','Thursday is the UPG Vizcaya Nursery Day!\r\n\r\nANNOUNCEMENT\r\nUPG Vizcaya needs YOU! We have vacancies for:\r\nInterns (College & HS)\r\nTeam Leaders\r\nVolunteer Staff\r\n