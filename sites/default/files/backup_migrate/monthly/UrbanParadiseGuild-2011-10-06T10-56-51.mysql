INSERT INTO `node_type` VALUES ('page','Page','node','A <em>page</em>, similar in form to a <em>story</em>, is a simple method for creating and displaying information that rarely changes, such as an \"About us\" section of a website. By default, a <em>page</em> entry does not allow visitor comments and is not featured on the site\'s initial home page.','','1','Title','1','Body','0','1','1','0','page'),('story','Story','node','A <em>story</em>, similar in form to a <em>page</em>, is ideal for creating and displaying content that informs or engages website visitors. Press releases, site announcements, and informal blog-like entries may all be created with a <em>story</em> entry. By default, a <em>story</em> entry is automatically featured on the site\'s initial home page, and provides the ability to post comments.','','1','Title','1','Body','0','1','1','0','story'),('webform','Webform','node','Create a new form or questionnaire accessible to users. Submission results and statistics are recorded and accessible to privileged users.','','1','Title','1','Body','0','1','1','0','webform'),('photo','Photo','node','','','1','Title','1','Caption','0','1','1','0',''),('news_story','News - story','node','News items that are published about Urban Paradise Guild','','1','Title','1','Body','0','1','1','0',''),('homepage','Homepage','node','The homepage.','','1','Title','1','Body','0','1','1','0',''),('testimonial','Testimonial / pullquote','node','a quote to be placed in the sidebar of the main content.','','1','Title (for internal use only)','1','Quote','0','1','1','0',''),('button','Button','node','Use this for the content and URL of a signup button, such as \"Sign up for updates\".','The title field is for the content management system only.','1','Title (not seen by user)','0','','0','1','1','0',''),('manual_page','Manual page','node','Content for the manual or wiki that will not be visible to the general public','','1','Title','1','Body','0','1','1','0',''),('press_release','News - Press release','node','News items that are published by Urban Paradise Guild','','1','Title','1','Body','0','1','1','0','');
INSERT INTO `node_type` VALUES ('announcement','News - Announcement','node','A piece of news put out by Urban Paradise Guild that are not as formal as a press release.','','1','Title','1','Body','0','1','1','0',''),('location','Location','node','A commonly-used venue for an event','','1','Title','1','Body','0','1','1','0',''),('event','Event - open','node','default type for an activity that\'s open to the public.','','1','Title','1','Body','0','1','1','0',''),('page_private','Wiki page','node','Used for internal staff wiki materials','','1','Title','1','Body','0','1','1','0','');
