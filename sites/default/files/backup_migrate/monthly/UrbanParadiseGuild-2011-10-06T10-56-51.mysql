INSERT INTO `node_revisions` VALUES ('488','490','84','Special Nursery Day El Portal - 10/16/11','MISSIONS\r\nTo Be Announced\r\n\r\nSCHEDULE\r\n9a-12p\r\n\r\nLEADERS\r\nFlorence - Chief Steward of UPG El Portal\r\nBonnie - UPG Montgomery Nursery Manager\r\n\r\nRSVP\r\nAbsolutely required.\r\nThe El Portal parts happen at private addresses (not published here), and the Montgomery nursery can be hard to find.\r\nI will send you our maps and my cell# to make sure you can join us.\r\n\r\nTo: florence@urban-paradise.org\r\nSubject: \"RSVP for El Portal to Montgomery 9/25/11\"\r\n\r\nBRING/WEAR/PREPARE\r\nPlease prepare yourself properly with...\r\n* Water Bottle (refillable)\r\n* Favorite sun-block & bug spray\r\n* Closed-toe shoes or boots\r\n* Gardening gloves (or use ours)\r\n* Dress for working in the heat\r\n\r\nTOOLS & Materials provided by UPG:\r\n* Endless Ice Water\r\n* Shovels\r\n* Soil\r\n* Mulch\r\n* Native plants\r\n* Veggies\r\n* Seeds\r\n* and other tools and materials as needed\r\n\r\nRSVP\r\nPlease RSVP to bonnie@urban-paradise.org if you plan to come. The nursery can be hard to find, so I will send you our map and my cell# to make sure you can join us.\r\nInclude the date and place.\r\nExample: \"RSVP for Montgomery 5/21/11\"\r\n\r\nLOCATION\r\nPlease see http://www.urban-paradise.org/Montgomery for driving directions and other important info.','MISSIONS\r\nTo Be Announced\r\n\r\nSCHEDULE\r\n9a-12p\r\n\r\nLEADERS\r\nFlorence - Chief Steward of UPG El Portal\r\nBonnie - UPG Montgomery Nursery Manager\r\n\r\nRSVP\r\nAbsolutely required.\r\nThe El Portal parts happen at private addresses (not published here), and the Montgomery nursery can be hard to find.\r\nI will send you our maps and my cell# to make sure you can join us.\r\n\r\nTo: florence@urban-paradise.org\r\nSubject: \"RSVP for El Portal to Montgomery 9/25/11\"\r\n\r\nBRING/WEAR/PREPARE\r\nPlease prepare yourself properly with...\r\n* Water Bottle (refillable)\r\n* Favorite sun-block & bug spray\r','','1317176045','1');
DROP TABLE IF EXISTS `node_type`;
